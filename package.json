{
	"name": "bring-artist-brand",
	"version": "0.0.1",
	"description": "",
	"scripts": {
		"build:autoprefix": "postcss dist/css/main.css -u autoprefixer -r",
		"build:css": "node-sass --output-style compressed \"./src/sass/main.scss\" \"./dist/assets/css/main.css\"",
		"build:css:dev": "node-sass --output-style compact --source-map true src/sass/main.scss dist/assets/css/main.css",
		"build": "npm run build:css && npm run build:autoprefix",
		"lint": "eslint ./src --cache --fix",
		"dev": "lite-server",
		"watch:css": "onchange './src/sass/components/*.scss' -- npm run build:css"
	},
	"husky": {
		"hooks": {
			"pre-push": "npm run lint"
		}
	},
	"devDependencies": {
		"@babel/cli": "^7.14.5",
		"@babel/core": "^7.14.6",
		"@babel/node": "^7.14.7",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/plugin-transform-classes": "^7.14.5",
		"@babel/preset-env": "^7.14.7",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-stage-2": "^7.8.3",
		"@babel/register": "^7.14.5",
		"autoprefixer": "^10.2.6",
		"browser-sync": "^2.27.3",
		"core-js": "^3.15.2",
		"eslint": "^7.29.0",
		"handlebars-loader": "^1.7.1",
		"husky": "^7.0.0",
		"lite-server": "^2.6.1",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"postcss": "^8.3.5",
		"webpack": "^5.41.1",
		"webpack-cli": "^4.7.2"
	},
	"dependencies": {
		"@babel/plugin-proposal-object-rest-spread": "^7.14.7",
		"array.prototype.flatmap": "^1.2.4",
		"babel-loader": "^8.2.2",
		"babel-preset-es2015": "^6.24.1",
		"babel-register": "^6.26.0",
		"boom": "^7.3.0",
		"glob-parent": "^6.0.0",
		"gulp-cli": "^2.3.0",
		"handlebars": "^4.7.7",
		"hoek": "^6.1.3",
		"jquery": "^3.6.0",
		"micromatch": "^4.0.4",
		"node-sass": "^6.0.1",
		"normalize.css": "^8.0.1",
		"postcss-cli": "^8.3.1",
		"rellax": "^1.12.1",
		"trim-newlines": "^4.0.2",
		"vinyl-fs": "^2.4.1",
		"wreck": "^14.2.0",
		"yargs": "^17.0.1",
		"yargs-parser": "^20.2.9"
	}
}
